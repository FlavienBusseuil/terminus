{
	"name": "terminus",
	"displayName": "terminus",
	"description": "Terminal Status",
	"version": "0.0.1",
	"publisher": "flavienbusseuil",
	"repository": "https://github.com/FlavienBusseuil/terminus",
	"engines": {
		"vscode": "^1.31.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Terminus configuration",
			"properties": {
				"terminus.matches": {
					"type": "array",
					"default": [
						{
							"display": "✕",
							"expression": "[Ee]rror|[Ee]xception"
						},
						{
							"display": "⚠︎",
							"expression": "[Ww]arning"
						},
						{
							"display": "↻",
							"expression": "[Rr]efresh(ing|ed){0,1}|[Rr]estart(ing|ed){0,1}|[Ss]tarted|[Rr]unning"
						}
					],
					"description": "Specifies the different matches."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.9.4",
		"prettier": "1.16.4",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3",
		"vscode": "^1.1.29"
	},
	"enableProposedAPI": true
}
